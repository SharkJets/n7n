---
import Layout from "../layouts/layout.astro";
import Setup from "../components/vue/Setup.vue";
import N8nDataViewer from "../components/vue/N8nDataViewer.vue";

const inputs = {
	key: 'X-N8N-API-KEY',
	token: import.meta.env.APIKEY,
	url: import.meta.env.APIURL
}
---
<Layout>
	<Setup client:load></Setup>
	<N8nDataViewer inputs={inputs} client:load></N8nDataViewer>
</Layout>